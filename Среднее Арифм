#include <iostream>
#include <time.h>
#include <math.h>
using namespace std;

int main()
{
    srand(time(NULL));

    const int SIZE = 10;
    int arr[SIZE];
    double A = 0, S = 0;
    for (int i = 0; i < SIZE; i++)
        {
            arr[i] = 1 + rand() % 10;
            cout << arr[i] << "\t";
        }
    cout << endl;
    for (int i = 0; i < SIZE; i++)
    {
        S += arr[i];
    }
    A = S / SIZE;
    cout << A;
}
